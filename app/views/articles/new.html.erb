<%= form_with scope: :article, url: articles_path, local: true do |form| %>

  <p>
    <%= form.label :titulo %><br>
    <%= form.text_field :title %>
  </p>
 
  <p>
    <%= form.label :bajada %><br>
    <%= form.text_area :subtitle %>
  </p>

  <p>
    <%= form.label :cuerpo %><br>
    <%= form.text_area :body %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>
<%= link_to 'Back', articles_path %>